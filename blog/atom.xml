---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>behaviac博客</title>
	<link href="http://tencentopen.github.io/behaviac/blog/atom.xml" rel="self" />
	<link href="http://tencentopen.github.io/behaviac/blog/" />
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://tencentopen.github.io/behaviac/</id>
	<author>
		<name>behaviac</name>
		<email>tencentgame@tencent.com</email>
	</author>
	{% for post in site.posts limit:10 %}
	<entry>
		<title>{{ post.title | xml_escape }}</title>
		<link href="http://tencentopen.github.io/behaviac{{ post.url }}" />
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>http://tencentopen.github.io/behaviac{{ post.id }}</id>
		<content type="html"><![CDATA[{{ post.content }}]]></content>
	</entry>
	{% endfor %}
</feed>