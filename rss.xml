---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>{{site.name}}</title>
	<subtitle>{{site.description}}</subtitle>
	<id>{{site.url}}{{site.baseurl}}/rss.xml</id>
	<updated>{{site.time}}</updated>
	<generator>Tencent behaviac Team</generator>
	<link href="http://tencentopen.github.io/behaviac/feed.xml" rel="self" type="application/atom+xml" />
	<link href="http://tencentopen.github.io/behaviac/" rel="alternate" type="text/html" />
	{% for post in site.posts %}
	<entry>
		<id>{{site.url}}{{site.baseurl}}{{post.url}}</id>
		<title type="text">{{post.title}}</title>
		<link rel="alternate" type="text/html" href="{{site.url}}{{site.baseurl}}{{post.url}}" />
		<link href="{{site.url}}{{site.baseurl}}{{post.url}}" rel="alternate" title="{{post.title }}" />
		<published>{{post.date}}</published>
		<updated>{{post.date}}</updated>
		<author>
			<name>{{post.author}}</name>
			<uri>https://github.com/{{post.author}}</uri>
		</author>
		<content type="html" xml:base="{{site.url}}{{site.baseurl}}{{post.url}}">
			<![DATA[
			{{post.content }}
			]]>
		</content>
		<summary type="html">
			<![DATA[ 
			{{post.content | truncatewords: 300 }}
			]]>
		</summary>
	</entry>
	{% endfor %}

	{% for post in site.docs %}
	<entry>
		<id>{{site.url}}{{site.baseurl}}{{post.url}}</id>
		<title type="text">{{post.title}}</title>
		<link rel="alternate" type="text/html" href="{{site.url}}{{site.baseurl}}{{post.url}}" />
		<link href="{{site.url}}{{site.baseurl}}{{post.url}}" rel="alternate" title="{{post.title }}" />
		<published>{{post.date}}</published>
		<updated>{{post.date}}</updated>
		<author>
			<name>{{post.author}}</name>
			<uri>https://github.com/{{post.author}}</uri>
		</author>
		<content type="html" xml:base="{{site.url}}{{site.baseurl}}{{post.url}}">
			<![DATA[
			{{post.content }}
			]]>
		</content>
		<summary type="html">
			<![DATA[ 
			{{post.content | truncatewords: 300 }}
			]]>
		</summary>
	</entry>
	{% endfor %}

</feed>
