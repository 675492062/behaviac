---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>{{site.name}}</title>
	<subtitle>{{site.description}}</subtitle>
	<id>{{site.url}}{{site.baseurl}}/rss.xml</id>
	<updated>{{site.time | date_to_xmlschema}}</updated>
	<generator>Tencent behaviac Team</generator>
	<link href="http://tencentopen.github.io/behaviac/feed.xml" rel="self" type="application/atom+xml" />
	<link href="http://tencentopen.github.io/behaviac/" rel="alternate" type="text/html" />
	{% for post in site.posts %}
	<entry>
		<title type="text">{{post.title}}</title>
		<link href="{{site.url}}{{site.baseurl}}{{post.url}}" rel="alternate" type="text/html" title="{{post.title | xml_escape}}" />
		<published>{{post.date | date_to_xmlschema}}</published>
		<updated>{{post.date | date_to_xmlschema }}</updated>
		<id>{{site.url}}{{site.baseurl}}{{post.url}}</id>
		<author>
			<name>{{post.author}}</name>
			<uri>https://github.com/{{post.author}}</uri>
		</author>
		<content type="html" xml:base="{{site.url}}{{site.baseurl}}{{post.url}}">
			<![CDATA[
			{{post.content | xml_escape}}
			]]>
		</content>
		<summary>
			<![CDATA[
			{{post.content | truncatewords: 300 | strip_html }}
			]]>
		</summary>
	</entry>
	{% endfor %}

	{% for post in site.docs %}
	<entry>
		<title type="text">{{post.title}}</title>
		<link href="{{site.url}}{{site.baseurl}}{{post.url}}" rel="alternate" type="text/html" title="{{post.title | xml_escape}}" />
		<published>{{post.date | date_to_xmlschema}}</published>
		<updated>{{post.date | date_to_xmlschema }}</updated>
		<id>{{site.url}}{{site.baseurl}}{{post.url}}</id>
		<author>
			<name>{{post.author}}</name>
			<uri>https://github.com/{{post.author}}</uri>
		</author>
		<content type="html" xml:base="{{site.url}}{{site.baseurl}}{{post.url}}">
			<![CDATA[
			{{post.content | xml_escape}}
			]]>
		</content>
		<summary>
			<![CDATA[
			{{post.content | truncatewords: 300 | strip_html }}
			]]>
		</summary>
	</entry>
	{% endfor %}

</feed>
